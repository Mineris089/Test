<Window x:Class="Testík.delWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Testík"
        mc:Ignorable="d"
        Title="delWin" Height="200" Width="250">
    <Grid>
        <Label Content="Opravdu chcete smazat tento záznam" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,40,0,0">
            <Button x:Name="AnoButton" Content="Ano" Width="75" Margin="10" Click="AnoButton_Click"/>
            <Button x:Name="NeButton" Content="Ne" Width="75" Margin="10" Click="NeButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
